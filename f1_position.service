[Unit]
Description=F1 Live Timing & Race Info API
After = network.target

[Service]
Type=simple
ExecStart=/usr/bin/env python3 /opt/scripts/f1-live-api/position.py
WorkingDirectory=/opt/scripts/f1-live-api
TimeoutStartSec=120
TimeoutStopSec=30
Restart=always
RestartSec=10
WatchdogSec=60
User=pi

[Install]
WantedBy=multi-user.target
